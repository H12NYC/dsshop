import request from '@/plugins/request'
export function getList(query) {
  return request({
    url: '{{ name }}',
    method: 'GET',
    params: query
  })
}
export function detail(id) {
  return request({
    url: '{{ name }}/' + id,
    method: 'GET'
  })
}
